package p1011;

public class J1011_06 {

	public static void main(String[] args) {
		String str = "홍길동";
		int kor = 100 ;
		int eng = 100 ;
		int math = 99 ;
		int total = kor+eng+math ;
		double avg = total/3.0 ;
		
		System.out.printf("이름\t국어\t영어\t수학\t합계\t평균\n");
		System.out.printf("%s\t%d\t%d\t%d\t%d\t%.3f\n", str, kor, eng, math, total, avg);
	}

}
