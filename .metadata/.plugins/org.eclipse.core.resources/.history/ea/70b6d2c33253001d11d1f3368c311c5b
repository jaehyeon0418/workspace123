package p1024;

import java.util.Scanner;

public class J1024_02 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		
		int num = 5 ;
		int []	a = null;
		Scanner scan = new Scanner(System.in);	

		a = new int[num];
		
		
		for (int i = 0; i < a.length; i++) {

			a[i] = 5 - i;
			
			System.out.println(a[i]);
			
			
			
		}
	
	}

}
